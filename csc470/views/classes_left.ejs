<!DOCTYPE html>
<html lang="en">
<head>
    <% include ./navbar %>
</head>
<body>
<div class="main">
		<div class="center addpadding">
		<h1>Classes Left to Take</h1>
		<table class="table table-striped table-condensed table-hover">
			<thead>
				<tr>
					<th>Class Name</th>
					<th>Registered</th>
					<th>Unregistered</th>
				</tr>
			<% for (var i = 0; i < all_classes.length; i++){ %>
				<% if (classes_taken.indexOf(all_classes[i]) == -1){ %>
				<tr> 
					<td> <%= all_classes[i]._name %> </td>
					<% if (classes_registered.indexOf(all_classes[i]) != -1){%> 
					<td > &#10004; </td> <td></td>
					<% } else { %>
					<td> </td> <td > &#10004; </td>
					<% } %>
				</tr>
				<% } %>
			<% } %>
		</table>
		</div>
</div>
</body>

